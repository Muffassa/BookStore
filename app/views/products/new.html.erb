<h1>New product page</h1>
<%= form_for @product do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.label :price %>
    <%= f.text_field :price %>
  </p>

  <p>
    <%= f.label :img_url %>
    <%= f.text_field :img_url %>
  </p>
  <div class="action">
    <%= f.submit "Create" %>
  </div>
<% end %>
